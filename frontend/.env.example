# API Configuration
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_API_TIMEOUT=30000

# App Configuration
REACT_APP_APP_NAME=PropertyRent
REACT_APP_APP_VERSION=1.0.0
REACT_APP_APP_DESCRIPTION=Property Rental Platform

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=true
REACT_APP_ENABLE_CHAT=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# External Services
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
REACT_APP_GOOGLE_ANALYTICS_ID=your_analytics_id
REACT_APP_SENTRY_DSN=your_sentry_dsn

# Development Configuration
GENERATE_SOURCEMAP=false
REACT_APP_ENABLE_LOGGING=true
REACT_APP_LOG_LEVEL=info

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=5242880
REACT_APP_ALLOWED_IMAGE_TYPES=image/jpeg,image/png,image/gif,image/webp
REACT_APP_ALLOWED_DOCUMENT_TYPES=application/pdf,application/msword

# Theme Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_ENABLE_DARK_MODE=true

# Pagination
REACT_APP_DEFAULT_PAGE_SIZE=10
REACT_APP_MAX_PAGE_SIZE=100

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_ENABLE_CACHING=true
